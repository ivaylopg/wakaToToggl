{
  "name": "wakatimetotoggl",
  "version": "0.1.0",
  "description": "API Adapter to post Wakatime stats as Toggl projects",
  "main": "index.js",
  "scripts": {
    "compile": "node_modules/nexe/bin/nexe"
  },
  "keywords": [
    "waktime",
    "time",
    "toggle",
    "tracking"
  ],
  "author": "Ivaylo Getov",
  "license": "MIT",
  "dependencies": {
    "argv": "0.0.2",
    "dotenv": "^2.0.0",
    "node-schedule": "^1.1.1",
    "request": "^2.75.0"
  },
  "devDependencies": {
    "nexe": "^1.1.2"
  },
  "nexe": {
    "input": "./index.js",
    "output": "./bin/wakaToToggl^$",
    "runtime": {
      "framework": "node",
      "version": "4.2.2",
      "ignoreFlags": true
    }
  }
}
